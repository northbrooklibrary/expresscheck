<?xml version="1.0" encoding="utf-8" ?>
<expresscheckinterface>

	<application>
		<backcolor color="#F0F2F1"></backcolor>
	</application>

	<!-- main menu buttons text and size customizations -->
	<buttons>
		<help color="#303F4D" size="12"></help>
		<backtocheckout color="#303F4D" size="12"></backtocheckout>
		<myaccount color="#303F4D" size="12"></myaccount>
		<printreceipt color="#303F4D" size="12" ></printreceipt>
		<printsummary color="#303F4D" size="12"></printsummary>
		<calculate color="#303F4D" size="12"></calculate>
		<exit color="#303F4D" size="12"></exit>
	</buttons>

	<form>
		<backcolor color="#F0F2F1"></backcolor>
		<forecolor color="#303F4D"></forecolor>
	</form>

	<dialog>
		<backcolor color="#F0F2F1"></backcolor>
		<forecolor color="#303F4D"></forecolor>
		<titlebar backcolor="#475268" forecolor="#FFFFFF" ></titlebar>
	</dialog>

	<headerpanel>
		<backcolor colorstart="#475268" colorend="#475268"></backcolor>
		<forecolor color="#475268"></forecolor>
		<!-- An empty filename uses the built-in image (200x78, transparent) -->
		<logo visible="true" filename="\\ccs-ts1\ExpressCheckConfig\img\logo.png"></logo>
		<helpbutton visible="false"></helpbutton>
	</headerpanel>

	<statuspanel>
		<backcolor colorstart="#475268" colorend="#475268"></backcolor>
		<forecolor color="#FFFFFF"></forecolor>
	</statuspanel>

	<welcomeform>
		<greeting color="#F0F2F1"></greeting>
		<instruction color="#303F4D"></instruction>
		<!-- An empty filename uses the built-in image (1003x616) -->
		<backgroundimage visible="false" filename=""></backgroundimage>
		<!-- An empty filename uses the built-in image (?x?) -->
		<instructionimage visible="true" filename="\\ccs-ts1\ExpressCheckConfig\img\welcome-instructionimage.png" width="387" height="218" top="192" left="288"></instructionimage>
		<!-- textalign values, "center", "left" or "right" -->
    <manualaccountlink top="564" left="23" width="956" textalign="center" forecolor="#303F4D"></manualaccountlink>
  </welcomeform>

  <loadingform>
    <greeting color="#303F4D"></greeting>
    <instruction color="#303F4D"></instruction>
    <!-- An empty filename uses the built-in image (1003x616) -->
    <backgroundimage visible="false" filename=""></backgroundimage>
    <!-- An empty filename uses the built-in image (?x?) -->
    <waitimage visible="true" filename="" width="32" height="32" top="150" left="485"></waitimage>
  </loadingform>

  <helpform>
		<!-- Default help video (720x480) -->
		<video filename="ECHelp.wmv" width="720" height="480" top="16" left="144"></video>
	</helpform>

	<languageform>
		<instruction color="#303F4D"></instruction>
	</languageform>

	<checkoutform>
		<forecolor color="#303F4D"></forecolor>
		<instructiontitlebar backcolor="#F0F2F1" forecolor="#303F4D"></instructiontitlebar>
		<instruction color="#303F4D"></instruction>
		<!-- An empty filename uses the built-in image (392x147) -->
		<instructionimage visible="true" filename="\\ccs-ts1\ExpressCheckConfig\img\checkout-instructionimage.png"></instructionimage>
<reminder headerforecolor ="#303F4D" headerbackcolor ="#6487DB" reminderforecolor="#303F4D" reminderbackcolor="#FAFAD2">

</reminder>
<gridview>

	<status successforecolor="" failureforecolor ="" ></status>
	<alternatingrow forecolor="" backcolor=""></alternatingrow>
	<background color="#FFFFFF"></background>
</gridview>
	</checkoutform>

</expresscheckinterface>
