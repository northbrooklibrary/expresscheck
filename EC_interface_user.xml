<?xml version="1.0" encoding="utf-8" ?>
<expresscheckinterface>

	<application>
		<backcolor color="#F5F5F5"></backcolor>
	</application>

	<!-- main menu buttons text and size customizations -->
	<buttons>
		<help color="#00739E" size="12"></help>
		<backtocheckout color="#00739E" size="12"></backtocheckout>
		<myaccount color="#00739E" size="12"></myaccount>
		<printreceipt color="#00739E" size="12" ></printreceipt>
		<printsummary color="#00739E" size="12"></printsummary>
		<calculate color="#00739E" size="12"></calculate>
		<exit color="#00739E" size="12"></exit>
	</buttons>

	<form>
		<backcolor color="#F5F5F5"></backcolor>
		<forecolor color="#333333"></forecolor>
	</form>

	<dialog>
		<backcolor color="#FFFFFF"></backcolor>
		<forecolor color="#333333"></forecolor>
		<titlebar backcolor="#00739E" forecolor="#FFFFFF" ></titlebar>
	</dialog>

	<headerpanel>
		<backcolor colorstart="#FFFFFF" colorend="#FFFFFF"></backcolor>
		<forecolor color="#FFFFFF"></forecolor>
		<!-- An empty filename uses the built-in image (200x78, transparent) -->
		<logo visible="true" filename="\\ccs-ts1\ExpressCheckConfig\img\logo.png"></logo>
		<helpbutton visible="false"></helpbutton>
	</headerpanel>

	<statuspanel>
		<backcolor colorstart="#00739E" colorend="#00739E"></backcolor>
		<forecolor color="#FFFFFF"></forecolor>
	</statuspanel>

	<welcomeform>
		<greeting color="#FFFFFF"></greeting>
		<instruction color="#00739E"></instruction>
		<!-- An empty filename uses the built-in image (1003x616) -->
		<backgroundimage visible="false" filename=""></backgroundimage>
		<!-- An empty filename uses the built-in image (?x?) -->
		<instructionimage visible="true" filename="\\ccs-ts1\ExpressCheckConfig\img\welcome-instructionimage.png" width="414" height="218" top="192" left="272"></instructionimage>
		<!-- textalign values, "center", "left" or "right" -->
    <manualaccountlink top="564" left="23" width="956" textalign="center" forecolor="#696969"></manualaccountlink>
  </welcomeform>

  <loadingform>
    <greeting color="#000080"></greeting>
    <instruction color="#696969"></instruction>
    <!-- An empty filename uses the built-in image (1003x616) -->
    <backgroundimage visible="false" filename=""></backgroundimage>
    <!-- An empty filename uses the built-in image (?x?) -->
    <waitimage visible="true" filename="" width="32" height="32" top="150" left="485"></waitimage>
  </loadingform>

  <helpform>
		<!-- Default help video (720x480) -->
		<video filename="ECHelp.wmv" width="720" height="480" top="16" left="144"></video>
	</helpform>

	<languageform>
		<instruction color="#000080"></instruction>
	</languageform>

	<checkoutform>
		<forecolor color="#333333"></forecolor>
		<instructiontitlebar backcolor="#FFFFFF" forecolor="#333333"></instructiontitlebar>
		<instruction color="#00739E"></instruction>
		<!-- An empty filename uses the built-in image (392x147) -->
		<instructionimage visible="true" filename="\\ccs-ts1\ExpressCheckConfig\img\checkout-instructionimage.png"></instructionimage>
<reminder headerforecolor ="#333333" headerbackcolor ="#6487DB" reminderforecolor="#333333" reminderbackcolor="#FAFAD2">

</reminder>
<gridview>

	<status successforecolor="" failureforecolor ="" ></status>
	<alternatingrow forecolor="" backcolor=""></alternatingrow>
	<background color="#FFFFFF"></background>
</gridview>
	</checkoutform>

</expresscheckinterface>
