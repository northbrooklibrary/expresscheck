<?xml version="1.0" encoding="utf-8" ?>
<expresscheckinterface>

	<application>
		<backcolor color="#F5F5F5"></backcolor>
		<messagebox>
			<!-- An empty wave filename and path uses the setting for the Windows Critical Stop sound (hand) -->
			<stop wave=""></stop>
		</messagebox>
	</application>

	<!-- main menu buttons text and size customizations -->
	<buttons>
		<help color="#6487db" size="12"></help>
		<backtocheckout color="#6487db" size="12"></backtocheckout>
		<myaccount color="#6487db" size="12"></myaccount>
		<printreceipt color="#6487db" size="12" ></printreceipt>
		<printsummary color="#6487db" size="12"></printsummary>
		<calculate color="#6487db" size="12"></calculate>
		<exit color="#6487db" size="12"></exit>
	</buttons>

	<form>
		<backcolor color="#F5F5F5"></backcolor>
		<forecolor color="#000000"></forecolor>
	</form>

	<dialog>
		<!-- B0C4DE Cornflower blue -->
		<backcolor color="#F5F5F5"></backcolor>
		<forecolor color="#000000"></forecolor>
		<titlebar backcolor="#778899" forecolor="#000000" ></titlebar>
	</dialog>

	<headerpanel>
		<backcolor colorstart="#000084" colorend="#6487db"></backcolor>
		<forecolor color="#FFFFFF"></forecolor>
		<!-- An empty filename uses the built-in image (200x78, transparent) -->
		<logo visible="true" filename=""></logo>
		<helpbutton visible="true"></helpbutton>
	</headerpanel>

	<statuspanel>
		<backcolor colorstart="#6487db" colorend="#000084"></backcolor>
		<forecolor color="#FFFFFF"></forecolor>
	</statuspanel>

	<welcomeform>
		<greeting color="#000080"></greeting>
		<instruction color="#696969"></instruction>
		<!-- An empty filename uses the built-in image (1003x616) -->
		<backgroundimage visible="false" filename=""></backgroundimage>
		<!-- An empty filename uses the built-in image (?x?) -->
		<instructionimage visible="true" filename="" width="470" height="304" top="192" left="272"></instructionimage>
		<!-- textalign values, "center", "left" or "right" -->
    <manualaccountlink top="564" left="23" width="956" textalign="center" forecolor="#696969"></manualaccountlink>
  </welcomeform>

  <loadingform>
    <greeting color="#000080"></greeting>
    <instruction color="#696969"></instruction>
    <!-- An empty filename uses the built-in image (1003x616) -->
    <backgroundimage visible="false" filename=""></backgroundimage>
    <!-- An empty filename uses the built-in image (?x?) -->
    <waitimage visible="true" filename="" width="32" height="32" top="150" left="485"></waitimage>
  </loadingform>

  <helpform>
		<!-- Default help video (720x480) -->
		<video filename="ECHelp.wmv" width="720" height="480" top="16" left="144"></video>
	</helpform>

	<languageform>
		<instruction color="#000080"></instruction>
	</languageform>

	<checkoutform>
		<forecolor color="#000000"></forecolor>
		<instructiontitlebar backcolor="#6487DB" forecolor="#000000"></instructiontitlebar>
		<instruction color="#000080"></instruction>
		<!-- An empty filename uses the built-in image (392x147) -->
		<instructionimage visible="true" filename=""></instructionimage>
		<rfidsecurityimage filename=""></rfidsecurityimage>
		<promptforreceipt filename=""></promptforreceipt>
		<mediadispenserimage filename=""></mediadispenserimage>
<reminder headerforecolor ="#000000" headerbackcolor ="#6487DB" reminderforecolor="#000000" reminderbackcolor="#FAFAD2">

</reminder>
<gridview>

	<status successforecolor="" failureforecolor ="" ></status>
	<alternatingrow forecolor="" backcolor=""></alternatingrow>
	<background color=""></background>
</gridview>
	</checkoutform>

	<patronaccountform>
		<forecolor color="#000000"></forecolor>
		<instruction color="#000080"></instruction>
		<!-- An empty filename uses the built-in image (392x147) -->
		<instructionimage filename=""></instructionimage>
		<initialtab index="0"></initialtab>

		<tab forecolor="#000000" backcolor="#B0C4DE" selectedcolor="#00008B" selectedunderline="true" selectedbold="true">
			<page backcolor="#B0C4DE">
				<header forecolor="#000000" backcolor="#D3D3D3"></header>
				<listitem backcolor="#FAFAD2" titlecolor="#00008B" labelcolor="#000000" datacolor="#800000"></listitem>
				<button forecolor="#000000" backcolor="#B0C4DE"></button>
			</page>
		</tab>

	</patronaccountform>

	<rfidinstructionform>
		<forecolor color="#000000"></forecolor>
		<instruction color="#000080"></instruction>
		<panel1backcolor color="#6487db"></panel1backcolor>
		<panel2backcolor color="#6487db"></panel2backcolor>
		<!-- An empty filename uses the built-in image (392x147) -->
		<instructionimage visible="true" filename=""></instructionimage>
	</rfidinstructionform>

</expresscheckinterface>
